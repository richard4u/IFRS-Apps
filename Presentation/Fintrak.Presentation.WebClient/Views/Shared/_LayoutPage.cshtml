                                                                                <img src="~/app/assets/img/subsub.png" />
                                                                                <span class="submenu-title">@subMenu.Alias</span>

                                                                                <i class="toggle-icon fa fa-angle-left"></i>
                                                                            </a>
                                                                        }*@

                                                                    @*<a href="#">
                                                                            <span class="text">@subMenu.Alias</span>
                                                                        </a>*@
                                                                }
                                                            </ul>
                                                        }
                                                    </li>
                                                }
                                            </ul>
                                        }
                                    </li>
                                }

                            </ul>
