                                                                @foreach (var subMenu in item.Children)
                                                                {
                                                                    if (subMenu.Children.Count <= 0 && !string.IsNullOrEmpty(subMenu.ActionUrl))
                                                                    {
                                                                        <li>
                                                                            <a @*ng-click="@ViewBag.Child= @subMenu.Alias"*@ data-ui-sref="@subMenu.ActionUrl">

                                                                                @if (!string.IsNullOrEmpty(subMenu.ImageUrl))
                                                                                {
